{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["esnext"]
  },
  "include": ["src/**/*"]
}
